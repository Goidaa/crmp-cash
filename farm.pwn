		SetPlayerData(playerid, P_RENT_CAR, INVALID_VEHICLE_ID);
	}

	SetPlayerJobLoadItems(playerid, 0);
	SetPlayerTempJob(playerid, TEMP_JOB_NONE);

	SetPlayerSpawnInit(playerid);

	return 1;
}

