forward CheckVehicleEngine(playerid);
public CheckVehicleEngine(playerid)
{
    if(!IsPlayerInAnyVehicle(playerid)) 
    {
        KillTimer(EngineTimer[playerid]);
        EngineTimer[playerid] = 0;
        return 0;
    }
    
    new vehicleid = GetPlayerVehicleID(playerid);
    
    if(EngineStalled[vehicleid])
    {
        new engine, lights, alarm, doors, bonnet, boot, objective;
        GetVehicleParamsEx(vehicleid, engine, lights, alarm, doors, bonnet, boot, objective);
       
        if(engine == 1)
        {
            SetVehicleParamsEx(vehicleid, 0, lights, alarm, doors, bonnet, boot, objective);
            SendClientMessage(playerid,  -1, "Требуетсч ремонт машины!");
        }
    }
    
    return 1;
}

stock StallVehicleEngine(vehicleid)
{
    new engine, lights, alarm, doors, bonnet, boot, objective;
    GetVehicleParamsEx(vehicleid, engine, lights, alarm, doors, bonnet, boot, objective);
    
    SetVehicleParamsEx(vehicleid, 0, lights, alarm, doors, bonnet, boot, objective);
    
    EngineStalled[vehicleid] = true;
    
    foreach(new i : Player)
    {
        if(IsPlayerInVehicle(i, vehicleid))
        {
            SendClientMessage(playerid, 0xCECECEFF, "Вы сломали машину!");
        }
    }
    
    return 1;
}

forward CrashHP_Process(playerid);
public CrashHP_Process(playerid)
{
    if(!IsPlayerInAnyVehicle(playerid)) return 1;

    new vehicleid = GetPlayerVehicleID(playerid);
    if(vehicleid == INVALID_VEHICLE_ID) return 1;

    new Float:vx, Float:vy, Float:vz;
    GetVehicleVelocity(vehicleid, vx, vy, vz);

    // скорость разницы
    new Float:speed = floatsqroot(vx*vx + vy*vy + vz*vz);
    new Float:last_speed = floatsqroot(last_vx[playerid]*last_vx[playerid] + last_vy[playerid]*last_vy[playerid] + last_vz[playerid]*last_vz[playerid]);

    new Float:diff = last_speed - speed;

    if(diff > 0.15)
    {
        new Float:hp;
        GetVehicleHealth(vehicleid, hp);

        hp -= diff * 300.0;

        if(hp < 270.0) hp = 270.0;
        if(hp > 1000.0) hp = 1000.0;

        SetVehicleHealth(vehicleid, hp);
        VehicleHP[vehicleid] = hp;
        
        if(hp <= 270.0 && !EngineStalled[vehicleid])
        {
            StallVehicleEngine(vehicleid);
        }
    }

    last_vx[playerid] = vx;
    last_vy[playerid] = vy;
    last_vz[playerid] = vz;
    LastSpeed[playerid] = speed;

    return 1;
}


CMD:repaircar(playerid, params[])
{
    if(!IsPlayerInAnyVehicle(playerid)) 
        return SendClientMessage(playerid, 0xFF0000AA, "Вы должны быть в транспортном средстве!");

    new vehicleid = GetPlayerVehicleID(playerid);
    
    SetVehicleHealth(vehicleid, 1000.0);
    VehicleHP[vehicleid] = 1000.0;
    
    if(EngineStalled[vehicleid])
    {
        EngineStalled[vehicleid] = false;
        ShowNotificationNew(playerid, 2, 5, 1, 1, "Двигатель восстановлен! Теперь можно завести машину.");
    }
    
    RepairVehicle(vehicleid);
    
    SendClientMessage(playerid, 0xCECECEFF, "Ваше транспортное средство было полностью отремонтировано!");
    return 1;
}
