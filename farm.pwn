CMD:larek(playerid, params[])
{
    new option;
    if(sscanf(params, "d", option))
    {
        // Показываем меню выбора
        SendClientMessage(playerid, -1, "Выберите вариант покупки:");
        SendClientMessage(playerid, -1, "/larek 1 - 120р = 10 голода");
        SendClientMessage(playerid, -1, "/larek 2 - 580р = 20 голода");
        SendClientMessage(playerid, -1, "/larek 3 - 230р = 10 голода");
        SendClientMessage(playerid, -1, "/larek 4 - 80р = 3 голода");
        SendClientMessage(playerid, -1, "/larek 5 - 320р = 15 голода");
        SendClientMessage(playerid, -1, "/larek 6 - 30р = 2 голода");
        return 1;
    }

    new Node:json = JSON_Object();
    new cost, satiety_amount;

    // Определяем стоимость и количество голода
    switch(option)
    {
        case 1: { cost = 120; satiety_amount = 10; }
        case 2: { cost = 580; satiety_amount = 20; }
        case 3: { cost = 230; satiety_amount = 10; }
        case 4: { cost = 80; satiety_amount = 3; }
        case 5: { cost = 320; satiety_amount = 15; }
        case 6: { cost = 30; satiety_amount = 2; }
        default:
        {
            JSON_SetInt(json, "o", 0);
            JSON_SetString(json, "error", "Неверный вариант. Используйте от 1 до 6");
            ShowPlayerGUI(playerid, 3, json);
            return 1;
        }
    }

    // Проверяем деньги
    if(GetPlayerMoney(playerid) < cost)
    {
        JSON_SetInt(json, "o", 0);
        JSON_SetString(json, "error", "У вас недостаточно денег!");
        ShowPlayerGUI(playerid, 3, json);
        return 1;
    }

    // Списание денег и пополнение голода
    GivePlayerMoney(playerid, -cost);
    player_satiety[playerid] += satiety_amount;

    // Проверка максимального значения
    if(player_satiety[playerid] > 100)
        player_satiety[playerid] = 100;

    // Успешная операция
    JSON_SetInt(json, "o", 1);
    JSON_SetInt(json, "m", GetPlayerMoney(playerid));
    JSON_SetInt(json, "satiety", player_satiety[playerid]);
    JSON_SetInt(json, "cost", cost);
    JSON_SetInt(json, "food_bought", satiety_amount);

    // Показываем уведомление
    new message[128];
    format(message, sizeof(message), "Вы купили %d единиц сытости за %d рублей", satiety_amount, cost);
    ShowNotificationNew(playerid, 3, 6, 0, 0, message, "");

    // Сохраняем в базу данных
    UpdatePlayerDatabaseInt(playerid, "satiety", player_satiety[playerid]);

    ShowPlayerGUI(playerid, 3, json);
    return 1;
}

