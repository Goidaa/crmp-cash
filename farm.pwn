forward OnPlayerSkinsLoaded(playerid);
public OnPlayerSkinsLoaded(playerid)
{
    new rows, fields;
    cache_get_data(rows, fields);

    if(rows > 0)
    {
        PlayerSkinCount[playerid] = 0;

        for(new i = 0; i < rows; i++)
        {
            if(i >= MAX_PLAYER_SKINS) break;

            PlayerSkins[playerid][i] = cache_get_field_content_int(i, "skin_id");
            PlayerSkinCount[playerid]++;
        }

        printf("Загружено %d скинов для игрока %d", PlayerSkinCount[playerid], playerid);
    }
    else
    {
        PlayerSkinCount[playerid] = 0;
    }
    return 1;
}


SavePlayerSkins(playerid)
{
    if(!IsPlayerConnected(playerid)) return 0;

    
    new delete_query[128];
    mysql_format(mysql, delete_query, sizeof(delete_query), "DELETE FROM player_skins WHERE player_id = %d", GetPlayerData(playerid, P_ACCOUNT_ID));
    mysql_tquery(mysql, delete_query);

    
    for(new i = 0; i < PlayerSkinCount[playerid]; i++)
    {
        new insert_query[256];
        mysql_format(mysql, insert_query, sizeof(insert_query),
            "INSERT INTO player_skins (player_id, skin_id, slot) VALUES (%d, %d, %d)",
            GetPlayerData(playerid, P_ACCOUNT_ID), PlayerSkins[playerid][i], i);
        mysql_tquery(mysql, insert_query);
    }

    return 1;
}


AddSkinToInventory(playerid, skinid)
{
    if(PlayerSkinCount[playerid] >= MAX_PLAYER_SKINS)
    {
        SendClientMessage(playerid, 0xFF0000AA, "Ваш инвентарь скинов полон!");
        return 0;
    }

    
    for(new i = 0; i < PlayerSkinCount[playerid]; i++)
    {
        if(PlayerSkins[playerid][i] == skinid)
        {
            SendClientMessage(playerid, 0xFF0000AA, "Этот скин уже есть в вашем инвентаре!");
            return 0;
        }
    }

    
    PlayerSkins[playerid][PlayerSkinCount[playerid]] = skinid;
    PlayerSkinCount[playerid]++;

    
    new insert_query[256];
    mysql_format(mysql, insert_query, sizeof(insert_query),
        "INSERT INTO player_skins (player_id, skin_id, slot) VALUES (%d, %d, %d)",
        GetPlayerData(playerid, P_ACCOUNT_ID), skinid, PlayerSkinCount[playerid]-1);
    mysql_tquery(mysql, insert_query);

    new msg[128];
    format(msg, sizeof(msg), "Скин %d добавлен в ваш инвентарь!", skinid);
    SendClientMessage(playerid, 0x00FF00AA, msg);

    return 1;
}


RemoveSkinFromInventory(playerid, slot)
{
    if(slot < 0 || slot >= PlayerSkinCount[playerid])
        return 0;

    
    new delete_query[256];
    mysql_format(mysql, delete_query, sizeof(delete_query),
        "DELETE FROM player_skins WHERE player_id = %d AND skin_id = %d",
        GetPlayerData(playerid, P_ACCOUNT_ID), PlayerSkins[playerid][slot]);
    mysql_tquery(mysql, delete_query);

    
    for(new i = slot; i < PlayerSkinCount[playerid]-1; i++)
    {
        PlayerSkins[playerid][i] = PlayerSkins[playerid][i+1];

        
        new update_query[256];
        mysql_format(mysql, update_query, sizeof(update_query),
            "UPDATE player_skins SET slot = %d WHERE player_id = %d AND skin_id = %d",
            i, GetPlayerData(playerid, P_ACCOUNT_ID), PlayerSkins[playerid][i]);
        mysql_tquery(mysql, update_query);
    }

    PlayerSkinCount[playerid]--;
    return 1;
}

