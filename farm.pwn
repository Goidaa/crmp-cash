        Dialog
        (
            playerid, DIALOG_FAMILY_CAR_RANG, DIALOG_STYLE_LIST,
            "{FFCD00}Транспорт семьи",
            fmt_text,
            "Выбрать", "Закрыть"
        );
    }
    cache_delete(result);
    return 1;
}

