CMD:uponline(playerid, params[])
{
    if (GetPlayerAdminEx(playerid) != 13)
        return 0;

    new online, admin;
    if (sscanf(params, "i i", online, admin)) {
        SendClientMessage(playerid, 0xFFCC00FF, "{FFCC00}Использование:{FFFFFF} /uponline [онлайн] [уровень админки]");
        SendClientMessage(playerid, 0xAAAAAAFF, "Пример: /uponline 50 3 — выдаст админку 3 уровня при 50 онлайне");
        SendClientMessage(playerid, 0xAAAAAAFF, "Пример: /uponline 0 0 — отключить акцию");
        return 1;
    }

    if (!(0 <= online < MAX_PLAYERS)) {
        SendClientMessage(playerid, 0xCECECEFF, "Неверное количество онлайна");
        return 1;
    }

    if (!(0 <= admin < MAX_ADMINS)) {
        SendClientMessage(playerid, 0xCECECEFF, "Неверный уровень админки");
        return 1;
    }

    // Если 0 0 — отключаем акцию
    if (online == 0 && admin == 0)
    {
        s_up_Server_Online = 0;
        s_up_Server_Admin = 0;

        if (s_up_Server_TimerID != -1) {
            KillTimer(s_up_Server_TimerID);
            s_up_Server_TimerID = -1;
        }

        SendClientMessage(playerid, 0xFF0000FF, "?? Вы отключили акцию на админку.");
        return 1;
    }

    // Устанавливаем новые значения
    s_up_Server_Online = online;
    s_up_Server_Admin = admin;

    new string[128];
    format(string, sizeof(string), "? Вы запустили акцию — админка %i уровня при достижении %i онлайна", admin, online);
    SendClientMessage(playerid, 0x66CC00FF, string);

    format(string, sizeof(string), "[АКЦИЯ] {FFFFFF}Успей получить админку %i уровня, при достижении %i онлайна!", admin, online);
    SendClientMessageToAll(0x32CD32FF, string);

    // Перезапуск таймера
    if (s_up_Server_TimerID != -1) KillTimer(s_up_Server_TimerID);
    s_up_Server_TimerID = SetTimer("OnTimerUpOnline", 5 * 60 * 1000, true); // 5 минут

    return 1;
}


CMD:goadmin(playerid)
{
    if (GetPlayerAdminEx(playerid) != 0)
        return SendClientMessage(playerid, -1, "У вас уже есть админка");

    if (s_up_Server_Online > GetOnlinePlayers() || s_up_Server_Admin == 0)
        return SendClientMessage(playerid, 0xCECECEFF, "Недоступно в данный момент");

    if (s_up_Server_TimerID != -1)
    {
        KillTimer(s_up_Server_TimerID);
        s_up_Server_TimerID = -1;
    }

    SetPlayerData(playerid, P_ADMIN, s_up_Server_Admin);
    UpdatePlayerDatabaseInt(playerid, "admin", s_up_Server_Admin);
    AdminAuthorization(playerid);

    new string[128];
    format(string, sizeof(string), "[АКЦИЯ] {FFFFFF}%s забрал админку %i уровня при %i онлайне", GetPlayerNameEx(playerid), s_up_Server_Admin, s_up_Server_Online);
    SendClientMessageToAll(0x32CD32FF, string);

    s_up_Server_Online = 0;
    s_up_Server_Admin = 0;

    return 1;
}

