new Float:strobe_timer[MAX_PLAYERS];

// Функция, которая будет вызываться каждую секунду для проверки мигания фарами
forward StrobeTimer(playerid);

// Команда для включения страбоскопов
CMD:strabs(playerid, params[])
{
    if(!IsPlayerInAnyVehicle(playerid))
    {
        SendClientMessage(playerid, COLOR_RED, "Вы должны находиться в машине!");
        return 1;
    }

    // Включаем мигание фарами
    SetVehicleParamsEx(GetPlayerVehicleID(playerid), 1);

    // Запускаем таймер для проверки мигания фарами
    strobe_timer[playerid] = SetTimerEx("StrobeTimer", 1000, true, "i", playerid);

    SendClientMessage(playerid, COLOR_GREEN, "Страбоскопы включены!");
    return 1;
}

// Функция для проверки мигания фарами
StrobeTimer(playerid)
{
    if(IsPlayerInAnyVehicle(playerid))
    {
        new vehicleid = GetPlayerVehicleID(playerid);

        if(GetVehicleParamsEx(vehicleid, 1) == 1)
        {
            SetVehicleParamsEx(vehicleid, 0);
        }
        else
        {
            SetVehicleParamsEx(vehicleid, 1);
        }
    }
    else
    {
        // Если игрок не находится в машине, останавливаем таймер
        KillTimer(strobe_timer[playerid]);
        strobe_timer[playerid] = 0;
    }
    return 1;
}


CMD:cmdTune(playerid, params[])
{
    new vehicleid = GetPlayerVehicleID(playerid);
    if(!IsPlayerInAnyVehicle(playerid) || !IsValidVehicle(vehicleid)) return SendClientMessage(playerid, COLOR_RED, "Вы должны находиться в автомобиле, чтобы установить прошивку.");

    new tuningtype;
    if(!strcmp(params, "sport", true)) tuningtype = 0;
    else if(!strcmp(params, "comfort", true)) tuningtype = 1;
    else if(!strcmp(params, "sport+", true)) tuningtype = 2;
    else if(!strcmp(params, "drift", true)) tuningtype = 3;
    else return SendClientMessage(playerid, COLOR_RED, "Неверный тип прошивки. Доступные типы: sport, comfort, sport+, drift.");

    if(g_CarTuning[vehicleid][tuningtype]) return SendClientMessage(playerid, COLOR_RED, "Эта прошивка уже установлена на ваш автомобиль.");

    // Здесь можно добавить проверки на наличие у игрока денег и т.д.

    // Устанавливаем прошивку на автомобиль
    OnSetCarTuning(playerid, vehicleid, tuningtype);

    return 1;
}

// Команда /untune
CMD:cmdUntune(playerid, params[])
{
    new vehicleid = GetPlayerVehicleID(playerid);
    if(!IsPlayerInAnyVehicle(playerid) || !IsValidVehicle(vehicleid)) return SendClientMessage(playerid, COLOR_RED, "Вы должны находиться в автомобиле, чтобы удалить прошивку.");

    new tuningtype;
    if(!strcmp(params, "sport", true)) tuningtype = 0;
    else if(!strcmp(params, "comfort", true)) tuningtype = 1;
    else if(!strcmp(params, "sport+", true)) tuningtype = 2;
    else if(!strcmp(params, "drift", true)) tuningtype = 3;
    else return SendClientMessage(playerid, COLOR_RED, "Неверный тип прошивки. Доступные типы: sport, comfort, sport+, drift.");

    if(!g_CarTuning[vehicleid][tuningtype]) return SendClientMessage(playerid, COLOR_RED, "Эта прошивка не установлена на ваш автомобиль.");

    // Удаляем прошивку с автомобиля
    OnRemoveCarTuning(playerid, vehicleid, tuningtype);

    return 1;
}


stock SetPlayerGPSPoint(playerid, Float:x, Float:y, Float:z, color = 0xFF0000FF)
{
