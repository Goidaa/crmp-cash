CMD:fixcartest(playerid, params[])
{
    new current_vehicle = GetPlayerOwnableCar(playerid);

    if (current_vehicle == INVALID_VEHICLE_ID)
    {
        SendClientMessage(playerid, 0x999999FF, "У вас нет загруженного транспорта");
        return 1;
    }

    if (!IsValidVehicle(current_vehicle))
    {
        
        SetPlayerData(playerid, P_OWNABLE_CAR, INVALID_VEHICLE_ID);

        
        launch[playerid] = 0;
        diski[playerid] = 0;
        fars[playerid] = 0;
        nitro[playerid] = 0;

        SendClientMessage(playerid, 0x66CC33FF, "{ffff00}| {ffffff}Невалидный транспорт удален! Теперь вы можете использовать /car нормально");
    }
    else
    {
        
        if (GetPlayerVehicleID(playerid) != current_vehicle)
        {
            if (UnloadPlayerOwnableCar(playerid, true))
            {
                SendClientMessage(playerid, 0x66CC33FF, "{ffff00}| {ffffff}Транспорт успешно выгружен!");
            }
            else
            {
                SendClientMessage(playerid, 0x999999FF, "Ошибка при выгрузке транспорта");
            }
        }
        else
        {
            SendClientMessage(playerid, 0x999999FF, "Вы находитесь в своем транспорте. Выйдите из него чтобы выгрузить");
        }
    }

    return 1;
}

