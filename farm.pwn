    if(ownable_car_id == -1)
        return SendClientMessage(playerid, -1, "Вы должны быть в своем личном автомобиле!");

    // Проверяем есть ли стробоскоп на этой машине в базе данных
    new query[128];
    mysql_format(mysql, query, sizeof(query), "SELECT stroboscope FROM ownable_cars WHERE id = %d", ownable_car_id);
    new Cache:cache = mysql_query(mysql, query);

    new has_stroboscope = 0;
    if(cache_num_rows() > 0)
    {
        has_stroboscope = cache_get_field_content_int(0, "stroboscope");
    }
    cache_delete(cache);

    if(has_stroboscope != 1)
        return SendClientMessage(playerid, -1, "Вы не установили стробоскопы на ваше транспортное средство! Установить стробоскопы в стайлинг-центре!");

