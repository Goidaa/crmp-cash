public timercar(playerid, second_timer)
{
    if(second_timer > 0) second_timer--; // если время не кончилось, то мы отнимаем 1 секунду
    else return logicacar(playerid); // если кончилось, то вызываем выгрузку

    new fmt_text[145];

    format(fmt_text, sizeof fmt_text, "Покиньте территорию парковки. \nВремени осталось %d сек.", second_timer); // обновляем текст
